import java.util.*;
class  UpperLowerCase
{
   static String uplow(String n)
	{  String newStr=""; 
	   for(int i=0; i<=n.length()-1; i++)
		{
		   newStr=newStr+ (String)(n.charAt(i)+32);
		}
		return newStr;
	}  


	public static void main(String[] args) 
	{   Scanner s=new Scanner(System.in);
		System.out.println("enter the String");
		String n=s.nextLine();
		System.out.println(uplow(n));
	}
}

import java.util.*;
class Binary 
{  

	public static void main(String[] args) 
	{   Scanner s=new Scanner(System.in);
		System.out.println("Please enter a number = ");
		int n=s.nextInt();
		System.out.println(display(n));
	}

	static boolean display(int org)
	{
		while(org>=1)
		{
			int d=org%10;
			org=org/10;
			if(d!=1 && d!=0)
			{
				return false;
			}
		}
		return true;
	}
}
